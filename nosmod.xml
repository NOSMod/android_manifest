<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <remote name="mod"
        fetch="https://github.com/NOSMod"
        sync-j="16" />


    <!-- general -->
    <project path="build/make" name="android_build_make" remote="mod" revision="p" >
        <copyfile src="core/root.mk" dest="Makefile" />
        <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
        <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
        <linkfile src="core" dest="build/core" />
        <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
        <linkfile src="target" dest="build/target" />
        <linkfile src="tools" dest="build/tools" />
    </project>

    <project path="bionic" name="android_bionic" remote="mod" revision="p"/>


    <!-- external -->
    <project path="external/selinux" name="android_external_selinux" remote="mod" revision="p" />
    <project path="external/perfetto" name="android_external_perfetto" remote="mod" revision="p" />

    <!-- system -->
    <project path="system/core" name="android_system_core" remote="mod" revision="p" />

    <!-- hardware -->
    <project path="hardware/qcom/audio-caf-msm8916" name="android_hardware_qcom_audio-caf-msm8916" remote="mod" revision="p" />
    <project path="hardware/qcom/media-caf-msm8916" name="android_hardware_qcom_media-caf-msm8916" remote="mod" revision="p" />
    <project path="hardware/qcom/display-caf-msm8916" name="android_hardware_qcom_display-caf-msm8916" remote="mod" revision="p" />
    <project path="hardware/qcom/bt-caf" name="android_hardware_qcom_bt-caf" remote="mod" revision="p-caf" />
    
    <!-- frameworks -->
    <project path="frameworks/av" name="android_frameworks_av" remote="mod" revision="p" />
    <project path="frameworks/base" name="android_frameworks_base" remote="mod" revision="p" />
    <project path="frameworks/native" name="android_frameworks_native" remote="mod" revision="p" />

    <!-- packages -->
    <project path="packages/apps/NitrogenSettings" name="android_packages_apps_NitrogenSettings" remote="mod" revision="p" />
    <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="mod" revision="p" />

    <!-- vendor -->
    <project path="vendor/nitrogen" name="android_vendor_nitrogen" remote="mod" revision="p" />
